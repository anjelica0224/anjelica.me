---
import { IoLogoGithub } from 'react-icons/io5'
---
<section class=" w-full-screen h-10 md:h-8 relative z-0">
        <hr class="border-gray-800 w-[60%] mx-auto mt-0 mb-2">
        <div class="flex justify-around items-start prose w-[65%] mx-auto md:text-2xl text-[11px] text-zinc-300 font-modern">
            <div class="flex justify-around sm:gap-2 gap-1 items-center">
                <span class="triangle"></span>
                <span class="">HOME</span>
            </div>
            <div class="flex justify-around sm:gap-2 gap-1 items-center">
                <span class="triangle"></span>
                <span class="">BLOG</span>
            </div>
            <div class="flex justify-around sm:gap-2 gap-1 items-center">
                <span class="triangle"></span>
                <span class="">PROJECTS</span>
            </div>
            <div class="flex justify-around sm:gap-2 gap-1 items-center">
                <span><IoLogoGithub /></span>
                <span class="">SOURCE</span>
            </div>
        </div>
        <hr class="border-gray-800 w-[60%] mx-auto mt-2 mb-2">

</section>